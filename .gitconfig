[user]
	name = Arni Magnusson
	email = thisisarni@gmail.com
[push]
	default = simple
[credential]
	helper = cache --timeout=3600
[gui]
	editor = emacs -Q
[diff]
	tool = kompare
[color]
	ui = auto
[cola]
	spellcheck = false
[merge]
	ff = false
[pull]
	ff = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
